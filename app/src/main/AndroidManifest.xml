<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appsmartlearn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NoticeWordItem"></activity>
        <activity android:name=".SetsActivity" />
        <activity android:name=".ScoreActivity" />
        <activity android:name=".Home" />
        <activity android:name=".QuestionActivity" />
        <activity
            android:name=".HomePage"
            android:label="HomePage"
            android:parentActivityName=".ActivityLogIn">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ActivityLogIn" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitySignUp"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ActivityLogIn"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ActivityNormalVocabList"
            android:label="600 Words Toeic"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomePage" />
        </activity>
        <activity
            android:name=".ActivityImgVocab"
            android:label="Learn Vocab With Image"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomePage" />
        </activity>
        <activity
            android:name=".ShowVideo"
            android:label="Video Lessons"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomePage" />
        </activity>
        <activity
            android:name=".NoticeWords"
            android:label="Video Lessons"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomePage" />
        </activity>
        <activity
            android:name=".UserProfile"
            android:parentActivityName=".HomePage" />
        <activity
            android:name=".EditUserProfile"
            android:parentActivityName=".UserProfile" />
        <activity
            android:name=".ResetPassword"
            android:parentActivityName=".UserProfile" />
    </application>

</manifest>